  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/toolkit": "8.3.1",
    "@types/lodash": "4.14.178",
    "tslib": "2.3.1"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@grafana/data": "8.3.1",
    "@grafana/runtime": "8.3.1",
    "@grafana/ui": "8.3.1"
  }
}

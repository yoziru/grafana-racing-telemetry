{
  "name": "grafana-simracing-telemetry-datasource",
  "version": "1.0.1",
  "description": "Plugin for fetching telemetry data from simracing titles",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "yarn lint --fix"
  },
  "author": "Grafana",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/toolkit": "8.3.1",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsdoc": "^37.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.0.6",
    "prettier": "^2.5.1",
    "tslib": "2.3.1"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@grafana/data": "8.3.1",
    "@grafana/runtime": "8.3.1",
    "@grafana/ui": "8.3.1"
  }
}

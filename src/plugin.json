{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "simracing-telemetry-datasource",
  "id": "grafana-simracing-telemetry-datasource",
  "metrics": true,
  "backend": true,
  "queryOptions": {"maxDataPoints": true},
  "executable": "gpx_simracing-telemetry-datasource",
  "info": {
    "description": "Plugin for fetching telemetry data from simracing titles",
    "author": {
      "name": "Grafana",
      "url": "https://grafana.com"
    },
    "keywords": [
      "simracing",
      "dirt rally"
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/grafana/grafana-starter-datasource-backend"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/grafana-starter-datasource-backend/blob/master/LICENSE"
      }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "dashboard",
      "name": "ACC Basic",
      "path": "dashboards/ACC/ACC_Basic.json"
    },
    {
      "type": "dashboard",
      "name": "ACC Advanced",
      "path": "dashboards/ACC/ACC_Advanced.json"
    },
    {
      "type": "dashboard",
      "name": "ACC Race Stats",
      "path": "dashboards/ACC/ACC_Race_Stats.json"
    },
    {
      "type": "dashboard",
      "name": "ACC Suspension",
      "path": "dashboards/ACC/ACC_Suspension.json"
    },
    {
      "type": "dashboard",
      "name": "iRacing Basic",
      "path": "dashboards/iRacing/iRacing_Basic.json"
    },
    {
      "type": "dashboard",
      "name": "Forza Basic",
      "path": "dashboards/Forza/Forza_Basic.json"
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=8.0.0",
    "plugins": []
  }
}
